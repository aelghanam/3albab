		</dependency>
		<dependency>
			<groupId>org.apache.derby</groupId>
			<artifactId>derby</artifactId>
			<version>10.8.1.2</version>
		</dependency>
		<!--dependency>
			<groupId>org.apache.activemq</groupId>
			<artifactId>activemq-all</artifactId>
