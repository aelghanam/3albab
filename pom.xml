	<groupId>com.boudy.albab</groupId>
	<artifactId>3albab</artifactId>
	<packaging>war</packaging>
	<version>1.0-SNAPSHOT</version>
	<name>3albab Maven Webapp</name>
	<url>http://maven.apache.org</url>
	<dependencies>
		<dependency>
